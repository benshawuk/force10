{
  "name": "force10-vite",
  "version": "0.1.1",
  "description": "Force10 Vite plugin â€” auto-generates route manifest",
  "license": "MIT",
  "author": "Ben Shaw",
  "repository": {
    "type": "git",
    "url": "https://github.com/benshawuk/force10.git",
    "directory": "packages/vite"
  },
  "keywords": ["vite", "vite-plugin", "inertia", "inertiajs", "laravel", "force10"],
  "type": "module",
  "main": "./dist/index.cjs",
